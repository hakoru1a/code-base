{
  "Routes": [
    {
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "${SERVICES_TLBIOMASS_HOST}",
          "Port": "${SERVICES_TLBIOMASS_PORT}"
        }
      ],
      "UpstreamPathTemplate": "/tlbiomass-api/{everything}",
      "UpstreamHttpMethod": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH"
      ],
      "SwaggerKey": "tlbiomass",
      "Priority": 0
    },
    {
      "DownstreamPathTemplate": "/swagger/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "${SERVICES_TLBIOMASS_HOST}",
          "Port": "${SERVICES_TLBIOMASS_PORT}"
        }
      ],
      "UpstreamPathTemplate": "/tlbiomass-api/swagger/{everything}",
      "UpstreamHttpMethod": [
        "GET"
      ],
      "SwaggerKey": "tlbiomass"
    },
    {
      "DownstreamPathTemplate": "/health",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "${SERVICES_TLBIOMASS_HOST}",
          "Port": "${SERVICES_TLBIOMASS_PORT}"
        }
      ],
      "UpstreamPathTemplate": "/tlbiomass-api/health",
      "UpstreamHttpMethod": [
        "GET"
      ]
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "tlbiomass",
      "Config": [
        {
          "Name": "TLBIOMASS API",
          "Version": "v1",
          "Url": "http://${GATEWAY_HOST}:${GATEWAY_PORT}/tlbiomass-api/swagger/v1/swagger.json"
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://${GATEWAY_HOST}:${GATEWAY_PORT}"
  }
}